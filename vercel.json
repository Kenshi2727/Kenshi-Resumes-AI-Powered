{
    "version": 2,
    "rewrites": [
        {
            "source": "/my-resume/:resumeId/view",
            "destination": "/index.html"
        },
        {
            "source": "/:path*",
            "destination": "/index.html"
        },
        {
            "source": "/service-worker.js",
            "destination": "/service-worker.js"
        }
    ]
}