{
    "version": 2,
    "builds": [
        {
            "src": "src/main.jsx",
            "use": "@vercel/static-build"
        }
    ],
    "rewrites": [
        {
            "source": "/my-resume/:resumeId/view",
            "destination": "/index.html"
        },
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ]
}